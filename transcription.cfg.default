title		=  Title
font_family = Helvetica
font_size	= 12


[interface_texts]
	current_row = Current row
	previous	= Previous
	save		= Save
	next		= Next
	first		= First
	load_sheet	= Load sheet
	last		= Last

	choose_sheet	= Choose yor sheet to work
	close_question 	= Are you sure you want to close this?


[fields]

	[[to_enter]]

		[[[field_1]]]
			column = A
			autocomplete = foo, bar
			position_gui = 2
			stay_next = True
		
		[[[field_2]]]
			column = B
			position_gui = 1
			stay_next = True

	[[permanent]]
	
		[[[field_1]]]
			column = A
			value = 123
		
		[[[field_2]]]
			column = A
			value = 456